pipeline {
    agent any 
    stages {
        stage('pull') { 
            steps {
                git 'https://github.com/miamorgithub/Fdec-B1-Project.git'
            }
        }
        stage('build') { 
            steps {
                 sh 'mvn clean package'
            }
        }
        stage('test') { 
            steps {
                 withSonarQubeEnv ('sonar') {
             sh ' mvn sonar:sonar '
                }
            }
        }
        stage('QualityCheck') { 
            steps {
                  timeout(time: 1, unit: 'HOURS') {
                         waitForQualityGate true
                }
            }
        }
        stage('deploy') { 
            steps {
                echo 'successfully deploy'
            }
        }
    }
}
